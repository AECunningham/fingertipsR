citHeader("To cite fingertips in publications use:")

citEntry(entry = "Misc",
         title        = "Public Health data made available with {fingertips}",
         institution  = "Public Health England",
         url          = "https://fingertips.phe.org.uk/",
         textVersion  =
             paste("Public Health England (2017).",
             "Public Health data made available with fingertips.",
             "URL https://fingertips.phe.org.uk/")
)
